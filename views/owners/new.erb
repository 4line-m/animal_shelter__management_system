<div class="owner-container">
  <h1>NEW OWNER</h1>
  <div class="row">


<form method="post" action="/owners">

  <label for="first-name-field">First Name:</label>
  <input
  type="text"
  name="first_name"
  id="first-name-field"
  />

  <br>

  <label for="last-name-field">Last Name:</label>
  <input
  type="text"
  name="last_name"
  id="last-name-field"
  />

  <br>

  <label for="address-field">Address:</label>
  <input type="text"
  name="address"
  id="address-field"
  />

  <br>

  <label for="email-address-field">Email:</label>
  <input
  type="text"
  name="email_address"
  id="email-address-field"
  />

  <br>

  <label for="phone-number-field">Phone Number:</label>
  <input
  type="text"
  name="phone_number"
  id="phone-number-field"
  />

  <br>

  <input type="submit" value="Add New Owner">
</form>

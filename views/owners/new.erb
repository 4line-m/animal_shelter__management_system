<form method="post" action="/owners">

  <label for="name-field">Name:</label>
  <input
  type="text"
  name="name"
  id="first-name-field"
  />

  <br>

  <label for="address-field">Address:</label>
  <input
  type="text"
  name="address"
  id="address-field"
  />

  <br>

  <label for="email-address-field">Email:</label>
  <input
  type="text"
  name="email_address"
  id="email-address-field"
  />

  <br>

  <label for="phone-number-field">Phone Number:</label>
  <input
  type="text"
  name="phone_number"
  id="phone-number-field"
  />

  <br>

  <input type="submit" value="Add New Owner">
</form>
